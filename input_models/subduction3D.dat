# subduction3D.dat
#	3-D Subduction setup - use CreatePhases.m to generate the input geometry  
#

# Define number of elements in x,y & z-direction @ coarsest level (should be uneven!)
nel_x						=	16     
nel_y						= 	1    
nel_z						= 	4
levels						= 	2			# number of multigrid levels

# Geometry
DimensionalUnits			=	1;			# Dimensionless (=0) or SI-dimensional units (=1)?
L							=	21.9e3		# Length (y-direction)
W							=	2800e3		# Width  (x-direction)
H							=	700e3		# Height (z-direction)
x_left						=	0e3		    # Left side of domain
y_front						=	0			# Front side of domain
z_bot						=	0			# Bottom of box

# Define model setup and noise
Setup.Model					=	6						# 0-Diapir, 1-Single Layer Fold, 2-Falling block, 3-Particle file (defined below), 4-multilayer detachment fold, 6-subduction w/sticky air
ParticleFilename			=   ParticlesInput3D.dat	# File that contains particle distribution
ampl2D						=	1e3 					# Amplitude of 2D sinuoidal perturbation [m]
ampl3D						=	0e2						# Amplitude of 3D sinuoidal perturbation [m]
amplNoise					=	0e2						# Amplitude of noise @ interface  [m]

# Define input/output and numerics-related stuff
OutputFile					=	Subduction3D_Output		
save_timesteps				=	1			# save every ? timesteps
time_end					=	1000000		# last timestep
CFL							=	1			# CFL, dt-based criterium 
dt_max						=	1e6  	    # Maximum timestep [years if dimensional]
		
# Boundary conditions
BC.Eyy						=	0e-15		# BG strainrate in y-direction
BC.Exx						=	0e-15		# BG Strainrate in x-direction
BC.LeftBound				=	1			# 0 - free surface, 1-free slip with BG strainrate Eyy, 2 - no slip
BC.RightBound				=	1			# 0 - free surface, 1-free slip with BG strainrate Eyy, 2 - no slip
BC.FrontBound				=	1			# 0 - free surface, 1-free slip with BG strainrate Exx, 2 - no slip
BC.BackBound				=	1			# 0 - free surface, 1-free slip with BG strainrate Exx, 2 - no slip
BC.LowerBound				=	1			# 0 - free surface, 1-free slip, 						2 - no slip
BC.UpperBound				=	1			# 0 - free surface, 1-free slip, 						2 - no slip
Temp_top	    			=   0;			# Temperature @ top
Temp_bottom   				=   1000;		# Temperature @ bottom; side BC's are flux-free

# Grid advection method
GridAdvectionMethod 		= 	0 		# 0-Fully Eulerian, 1-Fully Lagrangian, 2-ALE with remeshing @ surface layer
FactorSurfaceLayer  		= 	0.3 	# How thick is the surface layer (if option 2 is selected above) ?

# Define solver and set options for Uzawa
SolverType					=	5		# 2-KSP, 3-Direct [serial!],  4 - MUMPS, 5-GMG
UzawaSolver 				=	4		# 1-Schur/FGMRES;  2-Old way (CG);  3-Powell Hesteness iterations, 4- FC solver
MaximumDivergence  			=   1e-10    # Cutoff divergence
Uzawa_RelInnerAccuracy  	=   1e-8    # Cutoff inner error for preconditioning step in Uzawa iterations
Uzawa_MaxInnerIterations 	=   50		# Maximum number of inner iterations
  
<PetscOptionsStart>
	# Fine-tune solver options here (add command-line Petsc Options)
	#-vpt_element Q2Pm1_global 				# Type of element [Q2Pm1_global; Q2Pm1_local; Q1P0; Q1Q1]
	-vpt_element Q1P0 				# Type of element [Q2Pm1_global; Q2Pm1_local; Q1P0; Q1Q1]
	
	# some slab-setup specific parameters
	-Slab_AirThicknessFactor 0
	-MaximumSurfaceAngle 45
	-save_breakpoints 0
	-incr_breakpoints 100
	-AddRandomNoiseParticles 0		# no random noise added
	
	-Slab_AirThicknessFactor 0.4
	
	
	# Schur complement solver options:
	#-schur_ksp_type 					fgmres
	#-schur_ksp_type 					cg
        #-schur_ksp_norm_type 				UNPRECONDITIONED
       
       
	#-schur_ksp_monitor
	#-schur_ksp_converged_reason
	#-schur_pc_type 						bjacobi

	#-schur_ksp_atol						1.0e-12
	#-schur_ksp_rtol						1.0e-5

	
	# general GMG stuff:
	-gmg_ksp_atol 					1e-7
	-gmg_ksp_rtol 					1e-20
	-gmg_ksp_converged_reason

	-gmg_mg_levels_ksp_type 		fgmres
	-gmg_mg_levels_ksp_max_it 		4
	-gmg_mg_levels_pc_type 			bjacobi 
	
	# configure the coarse grid solver to be parallel direct 
	-gmg_mg_coarse_ksp_type 						preonly 
	-gmg_mg_coarse_pc_type 							lu
	-gmg_mg_coarse_pc_factor_mat_solver_package 	mumps
	
	
	
	#-----------------------------------------------------------------
	# setup stuff for the FC solver - GMG settings from above will still be usable

	-fc_ksp_type gcr			
        -fc_ksp_max_it 10

	-fc_ksp_rtol 1e-20
	-fc_ksp_atol 1e-4			# an approximation for the divergence

	-fc_ksp_monitor

	-fc_pc_type  			fieldsplit	
	-fc_pc_fieldsplit_type schur
	-fc_pc_fieldsplit_schur_factorization_type upper

 	-fc_fieldsplit_p_ksp_type preonly 
	-fc_fieldsplit_p_pc_type  jacobi

	#-fc_ksp_view
	
	
<PetscOptionsEnd>
  
    
# Phase distribution
ParticleInput 				=   1						# Use particles to track properties? 0-no; 1-yes
SaveTracers 				=   1             			# Write particles to disk?
NumPartX					=	3						# Particles/Cell in x-direction
NumPartY					=	3						# Particles/Cell in y-direction
NumPartZ					=	3						# Particles/Cell in z-direction

# Read initial grid from file?
InitialMeshFromFile 		=   0				#   In case you want to hand-modify the initial grid
 
# Properties that are defined over the whole domain:
Gravity						=	10;				# [m/s2] - gravitational acceleration 
PlasticityModel				=	0;				#  not implemented yet
Xi							=	0;				# [ ] - Shear heating factor - not implemented yet

# Define characteristic values, used if DimensionalUnits==1, otherwise ignored
Characteristic.Length		=	1e6			# [m]
Characteristic.Viscosity 	=	1e21		# [Pa.s]
Characteristic.Temperature	=	1000		# [K]
Characteristic.Stress      	=   1e6			# [Pa]	


# Define phase transitions for particles
num_phase_trans				=	0			# The number of phase transitions present

# Define material properties for all phases -------------------------------------------------
num_phases					=	3		# Total number of phases defined in code

	# Define properties of Mantle-------------------
	<MaterialStart>
		phase_id 			=	0
		attribute 			= 	VISCOSITY
		type 				= 	constant
		eta0       			= 	1.0e21	
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	0
		attribute 			= 	DENSITY
		type 				= 	temperature_dependent
		rho0        		= 	500
		alpha 				=	0
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	0
		attribute 			= 	ELASTICITY
		type 				= 	constant
		shear        		= 	5e10
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	0
		attribute 			= 	ENERGY
		type 				= 	constant
		ThermalConductivity	= 	3
		HeatCapacity		=	1050
		RadioactiveHeat		=	0
		ShearHeating		=	0
	<MaterialEnd>
	
	# Define properties of Slab -----------------
	<MaterialStart>
		phase_id 			=	1
		attribute 			= 	VISCOSITY
		type 				= 	constant
		eta0       			= 	1.0e24
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	1
		attribute 			= 	ELASTICITY
		type 				= 	constant
		shear        		= 	5e10
	<MaterialEnd>
	<MaterialStart>
		phase_id 			= 	1
		attribute 			= 	DENSITY
		type 				= 	temperature_dependent
		rho0        		= 	700
		alpha 				=	0
	<MaterialEnd>
	
	<MaterialStart>
		phase_id			= 	1
		attribute 			= 	ENERGY
		type 				= 	constant
		ThermalConductivity	= 	3
		HeatCapacity		=	1050
		RadioactiveHeat		=	0
		ShearHeating		=	0
	<MaterialEnd>

	# Define properties of Air -----------------
	<MaterialStart>
		phase_id 			=	2
		attribute 			= 	VISCOSITY
		type 				= 	constant
		eta0       			= 	1.0e21	
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	2
		attribute 			= 	ELASTICITY
		type 				= 	constant
		shear        		= 	5e10
	<MaterialEnd>
	<MaterialStart>
		phase_id 			= 	2
		attribute 			= 	DENSITY
		type 				= 	temperature_dependent
		rho0        		= 	500
		alpha 				=	0
	<MaterialEnd>
	
	<MaterialStart>
		phase_id			= 	2
		attribute 			= 	ENERGY
		type 				= 	constant
		ThermalConductivity	= 	3
		HeatCapacity		=	1050
		RadioactiveHeat		=	0
		ShearHeating		=	0
	<MaterialEnd>	
	
	
	# Define properties of DUMMY phase -----------------
	<MaterialStart>
		phase_id 			=	3
		attribute 			= 	VISCOSITY
		type 				= 	constant
		eta0       			= 	1.0e21	
	<MaterialEnd>
	<MaterialStart>
		phase_id			= 	3
		attribute 			= 	ELASTICITY
		type 				= 	constant
		shear        		= 	5e10
	<MaterialEnd>
	<MaterialStart>
		phase_id 			= 	3
		attribute 			= 	DENSITY
		type 				= 	temperature_dependent
		rho0        		= 	500
		alpha 				=	0
	<MaterialEnd>
	
	<MaterialStart>
		phase_id			= 	3
		attribute 			= 	ENERGY
		type 				= 	constant
		ThermalConductivity	= 	3
		HeatCapacity		=	1050
		RadioactiveHeat		=	0
		ShearHeating		=	0
	<MaterialEnd>
# End of defining material properties for all phases ----------------------------------------



