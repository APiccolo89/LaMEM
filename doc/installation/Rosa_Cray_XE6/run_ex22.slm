#!/bin/bash

# Job Name (do not use space!)
#SBATCH --job-name="petsc_ex22"

# Request the number of cores
#SBATCH --ntasks=32

# ask for the whole node
#SBATCH --ntasks-per-node=32

#SBATCH --time=00:02:00

# no space

EXEC=ex22

aprun -n $SLURM_NTASKS -N $SLURM_NTASKS_PER_NODE ./$EXEC -dmmg_nlevels 4 -ksp_monitor -ksp_view -da_grid_x 15 -da_grid_y 15 -da_grid_z 15 -mg_coarse_pc_type lu -mg_coarse_pc_factor_mat_solver_package mumps -log_summary 

exit
